<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension
         point="org.eclipse.ui.intro">
       <intro
             class="org.eclipse.ui.intro.config.CustomizableIntroPart"
             icon="icons/icon16.png"
             id="de.unidue.ecg.dialogScript.intro.intro">
       </intro>
       <introProductBinding
             introId="de.unidue.ecg.dialogScript.intro.intro"
             productId="de.unidue.ecg.dialogScript.product.product">
       </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            content="introContent.xml"
            id="de.unidue.ecg.dialogScript.intro.config"
            introId="de.unidue.ecg.dialogScript.intro.intro">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html">
            </implementation>
         </presentation>
      </config>
   </extension>  
   >
</plugin>
